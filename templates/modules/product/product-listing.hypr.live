{% with model|get_product_attribute("tenant~show-price") as showPriceAttr %}
    {% if showPriceAttr.values|first|prop("value") == 'view-on-login' %}
      {% if user.isAuthenticated %}
           <button class="mz-button add-to-cart {% if not model.purchasableState.isPurchasable %} is-disabled {% endif %}" data-productcode="{{model.productCode}}" {% if not model.purchasableState.isPurchasable %} disabled="disabled" {% endif %} > {{ labels.addToCart }} </button>
      {% endif %} 
      {% else %}
           <button class="mz-button add-to-cart {% if not model.purchasableState.isPurchasable %} is-disabled {% endif %}" data-productcode="{{model.productCode}}" {% if not model.purchasableState.isPurchasable %} disabled="disabled" {% endif %} > {{ labels.addToCart }} </button>
    {% endif %} 
{% endwith %}
{% with model|get_product_attribute("tenant~new-arrival") as newArrival %}
